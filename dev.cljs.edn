; Clojurescript compilation options - run this in a terminal:
;
; clj -m figwheel.main -b dev -r
;
{
 :watch-dirs ["src/cljc" "src/cljs"]
 :main                 "rs.main"
 :asset-path           "js"
 :source-map           true
 :source-map-timestamp true
 :pretty-print         true
 :output-dir           "resources/public/js"
 :output-to            "resources/public/js/main.js"
 :optimizations        :none
 :parallel-build       true
}